{
    "C_Cpp.default.includePath": [
        "${workspaceRoot}/Violet/include/c"
    ],
    "C_Cpp.default.cStandard": "c99",
    "files.associations": {
        "*.const": "json",
        "*.pmp": "json",
        "*.pms": "json",
        "*.h": "c",
        "*.cuh": "c",
        "typeinfo": "c",
        "functional": "c",
        "array": "c",
        "string": "c",
        "string_view": "c",
        "vector": "c",
        "memory": "c",
        "optional": "c",
        "system_error": "c",
        "thread": "c",
        "map": "c",
        "random": "c",
        "typeindex": "c",
        "bitset": "c",
        "chrono": "c",
        "variant": "c",
        "*.grit": "plaintext",
        "*.flags": "plaintext",
        "**/*.h": "c"
    },
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "workbench.editorAssociations": {
        "*.gba": "hexEditor.hexedit",
        "*.ipynb": "jupyter-notebook"
    },
    "editor.tabCompletion": "on",
    "diffEditor.codeLens": true
}